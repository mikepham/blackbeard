{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "baseUrl": ".",
    "declaration": false,
    "diagnostics": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "forceConsistentCasingInFileNames": true,
    "isolatedModules": false,
    "lib": [
      "es2015"
    ],
    "module": "commonjs",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "pretty": true,
    "removeComments": true,
    "rootDir": ".",
    "sourceMap": true,
    "strict": true,
    "strictNullChecks": true,
    "target": "es2015",
    "types": [
      "@types/chai",
      "@types/couchbase",
      "@types/debug",
      "@types/glob",
      "@types/gulp",
      "@types/gulp-copy",
      "@types/gulp-debug",
      "@types/gulp-load-plugins",
      "@types/gulp-mocha",
      "@types/gulp-shell",
      "@types/gulp-sourcemaps",
      "@types/gulp-tslint",
      "@types/lodash",
      "@types/lodash.merge",
      "@types/merge-stream",
      "@types/minimatch",
      "@types/mocha",
      "@types/node",
      "@types/node-fetch",
      "@types/node-schedule",
      "@types/orchestrator",
      "@types/q",
      "@types/react",
      "@types/react-dom",
      "@types/rimraf",
      "@types/source-map",
      "@types/tapable",
      "@types/through",
      "@types/uglify-js",
      "@types/vinyl",
      "@types/webpack",
      "@types/yargs"
    ]
  },
  "exclude": [
    "node_modules",
    "packages"
  ],
  "include": [
    "*.ts"
  ]
}