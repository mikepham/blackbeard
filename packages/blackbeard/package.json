    "include": [
      "src/**/*.ts"
    ],
    "instrument": true,
    "report-dir": "coverage",
    "reporter": [
      "html",
      "text",
      "text-summary"
    ],
    "sourceMap": true
  },
  "scripts": {
    "build": "gulp build",
    "clean": "gulp clean",
    "docker": "gulp docker",
    "test": "cross-env DEBUG= LCB_LOGLEVEL=5 NODE_ENV=testing NODE_PATH=. gulp test",
    "start": "clear && node index.js",
    "startdev": "clear && cross-env APPCMD=scheduler DEBUG=nativecode:blackbeard:* DOCKER_RUN=true ts-node src/index.ts",
    "prestartdev": "yarn run build",
    "docker-push": "./docker-push.sh",
    "predocker-push": "cross-env DOCKER_PUSH=yes yarn run docker",
    "docker-run": "cross-env APPCMD=scheduler DEBUG=nativecode:blackbeard:* DOCKER_RUN=true ./docker-run.sh",
    "predocker-run": "yarn run docker"
  }
