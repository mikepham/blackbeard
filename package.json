{
  "name": "nas-scripts",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Mike Pham <support@nativecode.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/debug": "^0.0.30",
    "@types/node": "^8.0.47",
    "@types/node-fetch": "^1.6.7",
    "@types/node-schedule": "^1.2.1",
    "cross-env": "^5.1.0",
    "rimraf": "^2.6.2",
    "ts-node": "^3.3.0",
    "tslint": "^5.8.0",
    "tslint-config-standard": "^6.0.1",
    "typescript": "^2.5.3"
  },
  "dependencies": {
    "async-throttle": "^1.1.0",
    "debug": "^3.1.0",
    "inversify": "^4.3.0",
    "node-fetch": "^1.7.3",
    "node-schedule": "^1.2.5",
    "reflect-metadata": "^0.1.10"
  },
  "scripts": {
    "build": "tsc --project tsconfig.json",
    "prebuild": "yarn run clean",
    "postbuild": "yarn run lint",
    "clean": "rimraf dist",
    "lint": "tslint src/**/*.ts",
    "scheduled": "ts-node src/run",
    "start": "node dist/scripts/run all",
    "startdev": "cross-env DEBUG=nativecode:* node dist/scripts/run all",
    "prestartdev": "yarn run build",
    "unmonitor-movies": "cross-env DEBUG=nativecode:*,-*:trace ts-node src/scripts/run unmonitor-movies",
    "preunmonitor-movies": "yarn run build",
    "unmonitor-shows": "cross-env DEBUG=nativecode:*,-*:trace ts-node src/scripts/run unmonitor-shows",
    "preunmonitor-shows": "yarn run build"
  }
}
